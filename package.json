{
  "name": "@dusanjovanov/vanilla-style",
  "version": "0.3.0",
  "description": "Vanilla CSS-in-JS library",
  "keywords": [
    "css-in-js",
    "vanilla",
    "emotion",
    "tailwind"
  ],
  "author": "Dusan Jovanov",
  "license": "MIT",
  "private": false,
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "source": "./src/index.ts",
  "main": "dist/index.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "15 kB"
    }
  ],
  "dependencies": {
    "@emotion/css": "^11.10.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "microbundle": "^0.15.1",
    "size-limit": "^8.1.0"
  }
}
